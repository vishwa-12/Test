  <wcm:element name="lost_bar_taskflow_text">Bar options</wcm:element>
  <wcm:element name="subscription_not_eligable_heading">This information isn't relevant to the number you're currently using</wcm:element>
  <wcm:element name="subscription_not_eligable_text">Please go to Account summary to choose another number or to see the services you can manage on this number.</wcm:element>
 <wcm:element name="remove_bar_subscription_link">&lt;ul class="link-list"&gt;&lt;li class="icon icon-arrow"&gt;&lt;a href="/webcenter/portal/myvodafone/accountsummary"&gt;Go to Account summary&lt;/a&gt;&lt;/li&gt;&lt;/ul&gt;</wcm:element>
  <wcm:element name="remove_bar_subscription_link_mobile">&lt;ul class="link-list"&gt;&lt;li class="icon icon-arrow"&gt;&lt;a href="/webcenter/portal/myvodafone/accountsummary"&gt;Go to Account summary&lt;/a&gt;&lt;/li&gt;&lt;/ul&gt;</wcm:element>
